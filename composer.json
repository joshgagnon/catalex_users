{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/joshgagnon/google2fa-laravel"
		}
	],
	"require": {
		"laravel/framework": "5.2.*",
		"lusitanian/oauth": "dev-master",
		"tijsverkoyen/css-to-inline-styles": "~2",
		"doctrine/dbal": "2.5.*",
		"omnipay/paymentexpress": "~2.0",
		"guzzlehttp/guzzle": "^6.0",
        "lucadegasperi/oauth2-server-laravel": "5.1.*",
        "predis/predis": "~1.0",
		"mews/captcha": "dev-master",
		"sentry/sentry-laravel": "^2.0@dev",
		"laravelcollective/html": "^5.2.0",
		"pragmarx/google2fa-laravel": "dev-hack-for-old-laravel",
		"bacon/bacon-qr-code": "dev-master"
	},
	"require-dev": {
		"symfony/dom-crawler": "~3.0",
		"symfony/css-selector": "~3.0",
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Tests\\Stub\\": "tests/stub/"
		}
	},
    "minimum-stability": "dev",
	"scripts": {
		"post-install-cmd": [

		],
		"post-update-cmd": [

		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
